--- # https://docs.ansible.com/ansible/latest/collections/ansible/builtin/package_module.html
- name: Add Package
  hosts: all
  tasks: 
  - name: Install telnet 
    ansible.builtin.package:
      name: "{{ item }}"
      state: present
    loop:
    - telnet
    - httpd 
    - elinks
    - lftp
    - unzip
    ignore_errors: true
